{
    "name": "@darraghor/nest-backend-libs",
    "version": "1.0.1",
    "license": "UNLICENSED",
    "description": "Some helpers for personal projects in nestjs",
    "homepage": "https://github.com/darraghoriordan/nest-backend-libs",
    "repository": {
        "type": "git",
        "url": "https://github.com/darraghoriordan/nest-backend-libs"
    },
    "engines": {
        "node": ">=18"
    },
    "type": "module",
    "files": [
        "dist",
        "package.json",
        "README.md"
    ],
    "author": {
        "name": "Darragh ORiordan",
        "email": "darragh.oriordan@gmail.com",
        "url": "https://darraghoriordan.com"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "keywords": [
        "nestjs"
    ],
    "bin": {
        "miller": "dist/cli/cli.js",
        "generate-client": "dist/open-api-generation/generate.sh"
    },
    "scripts": {
        "clean": "rm -Rf ./dist/",
        "build:ci": "yarn build",
        "build": "yarn clean && mkdir ./dist && tsc --project tsconfig.build.json && yarn copyOpenApiGenerationModule",
        "lint": "npx eslint -c .eslintrc.js --ext .ts './src/**/*.ts' './tests/**/*.ts' --no-error-on-unmatched-pattern",
        "test": "export NODE_OPTIONS=\"--max-old-space-size=8192\" && npx jest --runInBand --color --reporters=default --no-cache --coverage=false --passWithNoTests",
        "pre-commit": "npx lint-staged",
        "prettier": "prettier --write src/*",
        "prepare": "husky install",
        "release": "npx semantic-release",
        "semantic-release": "semantic-release",
        "copyOpenApiGenerationModule": "copyfiles --error --up 1 \"src/open-api-generation/**/*\" \"src/open-api-generation/*\" \"src/database-initialisation/*\" dist"
    },
    "peerDependencies": {
        "@nestjs/common": "^9.0.0",
        "@nestjs/config": "^2.2.0",
        "@nestjs/core": "^9.0.0",
        "@nestjs/passport": "^9.0.0",
        "@nestjs/platform-express": "^9.0.0",
        "@nestjs/swagger": "^6.0.0",
        "@nestjs/typeorm": "^9.0.0"
    },
    "dependencies": {
        "@nestjs/common": "^9.0.0",
        "@nestjs/config": "^2.2.0",
        "@nestjs/core": "^9.0.0",
        "@nestjs/passport": "^9.0.0",
        "@nestjs/platform-express": "^9.0.0",
        "@nestjs/swagger": "^6.1.4",
        "@nestjs/typeorm": "^9.0.0",
        "auth0": "3.1.2",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.0",
        "date-fns": "2.29.3",
        "dotenv": "16.0.3",
        "jwks-rsa": "3.0.1",
        "nodemailer": "6.9.1",
        "passport": "0.6.0",
        "passport-jwt": "4.0.1",
        "pg": "8.8.0",
        "reflect-metadata": "0.1.13",
        "res": "^0.4.0",
        "resource": "0.8.1",
        "rimraf": "4.1.2",
        "rxjs": "7.8.0",
        "stripe": "11.8.0",
        "swagger-ui-express": "4.6.0",
        "tslog": "3.3.4",
        "twitter-api-v2": "1.14.0",
        "typeorm": "^0.3.11"
    },
    "devDependencies": {
        "@commitlint/cli": "17.4.2",
        "@commitlint/config-conventional": "17.4.2",
        "@darraghor/eslint-plugin-nestjs-typed": "3.18.0",
        "@nestjs/bull": "0.6.2",
        "@nestjs/cli": "9.1.9",
        "@nestjs/schematics": "^9.0.4",
        "@semantic-release/changelog": "6.0.2",
        "@semantic-release/exec": "6.0.3",
        "@types/auth0": "2.35.9",
        "@types/express": "4.17.16",
        "@types/jest": "29.4.0",
        "@types/node": "18.11.18",
        "@types/nodemailer": "6.4.7",
        "@types/passport-jwt": "3.0.8",
        "@types/rimraf": "3.0.2",
        "@types/supertest": "2.0.12",
        "@typescript-eslint/eslint-plugin": "5.49.0",
        "@typescript-eslint/parser": "5.49.0",
        "bull": "4.10.2",
        "copyfiles": "2.4.1",
        "eslint": "8.32.0",
        "eslint-config-prettier": "8.6.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-jest": "27.2.1",
        "eslint-plugin-jsdoc": "39.6.9",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-promise": "6.1.1",
        "eslint-plugin-sonarjs": "0.18.0",
        "eslint-plugin-unicorn": "45.0.2",
        "husky": "8.0.3",
        "jest": "29.4.1",
        "jest-create-mock-instance": "2.0.0",
        "lint-staged": "13.1.0",
        "prettier": "2.8.3",
        "semantic-release": "20.1.0",
        "ts-jest": "29.0.5",
        "ts-loader": "9.4.2",
        "ts-node": "10.9.1",
        "tsconfig-paths": "4.1.2",
        "typescript": "4.9.4"
    },
    "lint-staged": {
        "*.ts": "eslint --cache --fix",
        "*.{ts,js,css,md}": "prettier --write"
    },
    "release": {
        "branches": [
            "main"
        ]
    }
}
