{
    "name": "@darraghor/nest-backend-libs",
    "version": "1.0.1",
    "license": "SEE LICENSE IN LICENSE.md",
    "description": "Some helpers for personal projects in nestjs",
    "homepage": "https://github.com/darraghoriordan/nest-backend-libs",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/darraghoriordan/nest-backend-libs"
    },
    "engines": {
        "node": ">=22.13.1"
    },
    "files": [
        "dist",
        "package.json",
        "README.md"
    ],
    "author": {
        "name": "Darragh ORiordan",
        "email": "darragh.oriordan@gmail.com",
        "url": "https://darraghoriordan.com"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.js",
    "exports": {
        "import": "./dist/index.js"
    },
    "keywords": [
        "nestjs"
    ],
    "bin": {
        "miller": "dist/cli/cli.js",
        "generate-client": "dist/open-api-generation/generate.sh"
    },
    "scripts": {
        "clean": "rm -Rf ./dist/",
        "build:ci": "pnpm run build",
        "build": "pnpm run clean && mkdir ./dist && tsc --project tsconfig.build.json && pnpm run copyOpenApiGenerationModule",
        "lint": "pnpm exec eslint",
        "lintone": "pnpm exec eslint  --no-error-on-unmatched-pattern",
        "test": "export NODE_OPTIONS=\"--max-old-space-size=8192\" && npx jest --runInBand --color --reporters=default --no-cache --coverage=false --passWithNoTests",
        "pre-commit": "npx lint-staged",
        "prettier": "prettier --write src/*",
        "prepare": "husky",
        "knip": "knip",
        "release": "npx semantic-release",
        "semantic-release": "semantic-release",
        "copyOpenApiGenerationModule": "copyfiles --error --up 1 \"src/open-api-generation/**/*\" \"src/open-api-generation/*\" \"src/database-initialisation/*\" dist"
    },
    "peerDependencies": {
        "@nestjs/axios": "4.0.0",
        "@nestjs/bull": "11.0.2",
        "@nestjs/common": "11.0.10",
        "@nestjs/config": "4.0.0",
        "@nestjs/core": "11.0.10",
        "@nestjs/microservices": "11.0.10",
        "@nestjs/passport": "11.0.5",
        "@nestjs/platform-express": "11.0.10",
        "@nestjs/swagger": "11.0.3",
        "@nestjs/terminus": "11.0.0",
        "@nestjs/typeorm": "11.0.0",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.1",
        "reflect-metadata": "0.2.2",
        "typeorm": "0.3.20"
    },
    "dependencies": {
        "@keyv/redis": "4.3.2",
        "@nestjs/axios": "4.0.0",
        "@nestjs/bull": "11.0.2",
        "@nestjs/cache-manager": "3.0.1",
        "@nestjs/common": "11.0.12",
        "@nestjs/config": "4.0.2",
        "@nestjs/core": "11.0.12",
        "@nestjs/microservices": "11.0.12",
        "@nestjs/passport": "11.0.5",
        "@nestjs/platform-express": "11.0.12",
        "@nestjs/swagger": "11.1.0",
        "@nestjs/terminus": "11.0.0",
        "@nestjs/typeorm": "11.0.0",
        "add": "2.0.6",
        "auth0": "4.21.0",
        "axios": "1.8.4",
        "bull": "4.16.5",
        "cache-manager": "6.4.1",
        "chalk": "5.4.1",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.1",
        "date-fns": "4.1.0",
        "dotenv": "16.4.7",
        "helmet": "8.1.0",
        "jwks-rsa": "3.2.0",
        "nanoid": "5.1.5",
        "nestjs-pino": "4.4.0",
        "nodemailer": "6.10.0",
        "passport": "0.7.0",
        "passport-headerapikey": "1.2.2",
        "passport-jwt": "4.0.1",
        "pino-http": "10.4.0",
        "pino-pretty": "13.0.0",
        "redis": "4.7.0",
        "reflect-metadata": "0.2.2",
        "rimraf": "6.0.1",
        "rxjs": "7.8.2",
        "stripe": "17.7.0",
        "swagger-ui-express": "5.0.1",
        "twitter-api-v2": "1.22.0",
        "typeorm": "0.3.26"
    },
    "devDependencies": {
        "@commitlint/cli": "19.8.0",
        "@commitlint/config-conventional": "19.8.0",
        "@darraghor/eslint-plugin-nestjs-typed": "6.4.3",
        "@eslint/js": "9.23.0",
        "@nestjs/cli": "11.0.5",
        "@nestjs/schematics": "^11.0.2",
        "@semantic-release/changelog": "6.0.3",
        "@semantic-release/exec": "7.0.3",
        "@types/auth0": "3.3.10",
        "@types/express": "5.0.1",
        "@types/jest": "29.5.14",
        "@types/node": "22.13.14",
        "@types/nodemailer": "6.4.17",
        "@types/passport-jwt": "4.0.1",
        "@types/supertest": "6.0.3",
        "@types/yargs": "17.0.33",
        "@typescript-eslint/eslint-plugin": "8.28.0",
        "@typescript-eslint/parser": "8.28.0",
        "copyfiles": "2.4.1",
        "eslint": "9.23.0",
        "eslint-config-prettier": "10.1.1",
        "eslint-plugin-jest": "28.11.0",
        "eslint-plugin-sonarjs": "3.0.2",
        "eslint-plugin-unicorn": "58.0.0",
        "globals": "16.0.0",
        "husky": "9.1.7",
        "jest": "29.7.0",
        "jest-create-mock-instance": "2.0.0",
        "knip": "5.46.2",
        "lint-staged": "15.5.0",
        "prettier": "3.5.3",
        "semantic-release": "24.2.3",
        "ts-jest": "29.3.0",
        "ts-loader": "9.5.2",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.8.2",
        "typescript-eslint": "8.28.0",
        "yargs": "17.7.2"
    },
    "lint-staged": {
        "*.ts": "eslint --cache --fix",
        "*.{ts,js,css,md}": "prettier --write"
    },
    "release": {
        "branches": [
            "main"
        ]
    }
}
