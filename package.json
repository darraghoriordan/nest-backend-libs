{
    "name": "@darraghor/nest-backend-libs",
    "version": "1.0.1",
    "description": "Some helpers for personal projects",
    "homepage": "https://github.com/darraghoriordan/nest-backend-libs",
    "repository": {
        "type": "git",
        "url": "https://github.com/darraghoriordan/nest-backend-libs"
    },
    "engines": {
        "node": ">=12"
    },
    "files": [
        "dist",
        "package.json",
        "README.md"
    ],
    "author": {
        "name": "Darragh ORiordan",
        "email": "darragh.oriordan@gmail.com",
        "url": "https://darraghoriordan.com"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "keywords": [
        "nestjs"
    ],
    "license": "ISC",
    "scripts": {
        "clean": "rm -Rf ./dist/",
        "build:ci": "yarn build",
        "build": "yarn clean && mkdir ./dist && tsc --project tsconfig.build.json && yarn copyOpenApiGenerationModule",
        "lint": "npx eslint -c .eslintrc.js --ext .ts './src/**/*.ts' './tests/**/*.ts' --no-error-on-unmatched-pattern",
        "test": "export NODE_OPTIONS=\"--max-old-space-size=8192\" && npx jest --runInBand --color --reporters=default --no-cache --coverage=false --passWithNoTests",
        "pre-commit": "npx lint-staged",
        "prettier": "prettier --write src/*",
        "prepare": "husky install",
        "release": "npx semantic-release",
        "semantic-release": "semantic-release",
        "copyOpenApiGenerationModule": "copyfiles --error --up 1 \"src/open-api-generation/**/*\" \"src/open-api-generation/*\" \"src/database-initialisation/*\" dist"
    },
    "dependencies": {
        "@nestjs/common": "7.6.18",
        "@nestjs/config": "0.6.3",
        "@nestjs/core": "7.6.18",
        "@nestjs/passport": "7.1.6",
        "@nestjs/platform-express": "7.6.18",
        "@nestjs/swagger": "4.8.2",
        "@nestjs/typeorm": "7.1.5",
        "auth0": "2.37.0",
        "class-transformer": "0.5.1",
        "class-validator": "0.13.2",
        "date-fns": "2.27.0",
        "dotenv": "10.0.0",
        "jwks-rsa": "2.0.5",
        "nodemailer": "6.7.2",
        "passport": "0.5.0",
        "passport-jwt": "4.0.0",
        "pg": "8.7.1",
        "reflect-metadata": "0.1.13",
        "res": "^0.4.0",
        "resource": "0.8.1",
        "rimraf": "3.0.2",
        "rxjs": "7.4.0",
        "swagger-ui-express": "4.2.0",
        "tslog": "3.3.0",
        "typeorm": "^0.2.37"
    },
    "devDependencies": {
        "@commitlint/cli": "15.0.0",
        "@commitlint/config-conventional": "15.0.0",
        "@darraghor/eslint-plugin-nestjs-typed": "3.5.7",
        "@nestjs/cli": "7.6.0",
        "@nestjs/schematics": "^7.3.1",
        "@semantic-release/changelog": "5.0.1",
        "@semantic-release/exec": "5.0.0",
        "@types/auth0": "2.34.7",
        "@types/express": "4.17.13",
        "@types/jest": "27.0.3",
        "@types/node": "16.11.11",
        "@types/nodemailer": "6.4.4",
        "@types/passport-jwt": "3.0.6",
        "@types/rimraf": "3.0.2",
        "@types/supertest": "2.0.11",
        "@typescript-eslint/eslint-plugin": "5.1.0",
        "@typescript-eslint/parser": "5.1.0",
        "copyfiles": "2.4.1",
        "eslint": "8.3.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-jest": "25.3.0",
        "eslint-plugin-jsdoc": "37.1.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-promise": "5.2.0",
        "eslint-plugin-sonarjs": "0.11.0",
        "eslint-plugin-unicorn": "39.0.0",
        "husky": "7.0.4",
        "jest": "27.4.2",
        "jest-create-mock-instance": "2.0.0",
        "lint-staged": "12.1.2",
        "prettier": "2.5.0",
        "semantic-release": "17.4.3",
        "ts-jest": "27.0.7",
        "ts-loader": "9.2.6",
        "ts-node": "10.4.0",
        "tsconfig-paths": "3.12.0",
        "typescript": "4.5.2"
    },
    "lint-staged": {
        "*.ts": "eslint --cache --fix",
        "*.{ts,js,css,md}": "prettier --write"
    },
    "release": {
        "branches": [
            "main"
        ]
    }
}
