{
    "name": "@darraghor/nest-backend-libs",
    "version": "1.0.1",
    "license": "SEE LICENSE IN LICENSE.md",
    "description": "Some helpers for personal projects in nestjs",
    "homepage": "https://github.com/darraghoriordan/nest-backend-libs",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/darraghoriordan/nest-backend-libs"
    },
    "engines": {
        "node": ">=22.12.0"
    },
    "files": [
        "dist",
        "package.json",
        "README.md"
    ],
    "author": {
        "name": "Darragh ORiordan",
        "email": "darragh.oriordan@gmail.com",
        "url": "https://darraghoriordan.com"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.js",
    "exports": {
        "import": "./dist/index.js"
    },
    "keywords": [
        "nestjs"
    ],
    "bin": {
        "miller": "dist/cli/cli.js",
        "generate-client": "dist/open-api-generation/generate.sh"
    },
    "scripts": {
        "clean": "rm -Rf ./dist/",
        "build:ci": "pnpm run build",
        "build": "pnpm run clean && mkdir ./dist && tsc --project tsconfig.build.json && yarn copyOpenApiGenerationModule",
        "lint": "npx eslint -c .eslintrc.cjs './src/**/*.ts' './tests/**/*.ts' --no-error-on-unmatched-pattern",
        "lintone": "npx eslint -c .eslintrc.cjs --no-error-on-unmatched-pattern",
        "test": "export NODE_OPTIONS=\"--max-old-space-size=8192\" && npx jest --runInBand --color --reporters=default --no-cache --coverage=false --passWithNoTests",
        "pre-commit": "npx lint-staged",
        "prettier": "prettier --write src/*",
        "prepare": "husky",
        "knip": "knip",
        "release": "npx semantic-release",
        "semantic-release": "semantic-release",
        "copyOpenApiGenerationModule": "copyfiles --error --up 1 \"src/open-api-generation/**/*\" \"src/open-api-generation/*\" \"src/database-initialisation/*\" dist"
    },
    "peerDependencies": {
        "@nestjs/axios": "2.0.0",
        "@nestjs/bull": "0.6.3",
        "@nestjs/common": "9.3.12",
        "@nestjs/config": "2.3.1",
        "@nestjs/core": "9.3.12",
        "@nestjs/microservices": "9.3.12",
        "@nestjs/passport": "9.0.3",
        "@nestjs/platform-express": "9.3.12",
        "@nestjs/swagger": "6.2.1",
        "@nestjs/terminus": "9.2.1",
        "@nestjs/typeorm": "9.0.1",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.0",
        "reflect-metadata": "0.1.13",
        "typeorm": "0.3.12"
    },
    "dependencies": {
        "@nestjs/axios": "3.1.3",
        "@nestjs/bull": "10.2.3",
        "@nestjs/cache-manager": "2.3.0",
        "@nestjs/common": "10.4.15",
        "@nestjs/config": "3.3.0",
        "@nestjs/core": "10.4.15",
        "@nestjs/microservices": "10.4.15",
        "@nestjs/passport": "10.0.3",
        "@nestjs/platform-express": "10.4.15",
        "@nestjs/swagger": "8.1.0",
        "@nestjs/terminus": "10.2.3",
        "@nestjs/typeorm": "10.0.2",
        "add": "2.0.6",
        "auth0": "4.15.0",
        "axios": "1.7.9",
        "bull": "4.16.5",
        "cache-manager": "5.7.6",
        "cache-manager-redis-yet": "5.1.5",
        "chalk": "5.4.1",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.1",
        "date-fns": "4.1.0",
        "dotenv": "16.4.7",
        "helmet": "8.0.0",
        "jwks-rsa": "3.1.0",
        "nanoid": "5.0.9",
        "nestjs-pino": "4.2.0",
        "nodemailer": "6.9.16",
        "passport": "0.7.0",
        "passport-headerapikey": "1.2.2",
        "passport-jwt": "4.0.1",
        "pino-http": "10.3.0",
        "pino-pretty": "13.0.0",
        "redis": "4.7.0",
        "reflect-metadata": "0.2.2",
        "rimraf": "6.0.1",
        "rxjs": "7.8.1",
        "stripe": "17.6.0",
        "swagger-ui-express": "5.0.1",
        "twitter-api-v2": "1.19.0",
        "typeorm": "0.3.20"
    },
    "devDependencies": {
        "@commitlint/cli": "19.6.1",
        "@commitlint/config-conventional": "19.6.0",
        "@darraghor/eslint-plugin-nestjs-typed": "5.0.30",
        "@nestjs/cli": "10.4.9",
        "@nestjs/schematics": "^10.2.3",
        "@semantic-release/changelog": "6.0.3",
        "@semantic-release/exec": "6.0.3",
        "@types/auth0": "3.3.10",
        "@types/express": "5.0.0",
        "@types/jest": "29.5.14",
        "@types/node": "22.10.2",
        "@types/nodemailer": "6.4.17",
        "@types/passport-jwt": "4.0.1",
        "@types/supertest": "6.0.2",
        "@types/yargs": "17.0.33",
        "@typescript-eslint/eslint-plugin": "8.19.0",
        "@typescript-eslint/parser": "8.19.0",
        "copyfiles": "2.4.1",
        "eslint": "9.17.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-jest": "28.10.0",
        "eslint-plugin-jsdoc": "50.6.1",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-promise": "7.2.1",
        "eslint-plugin-sonarjs": "3.0.1",
        "eslint-plugin-unicorn": "56.0.1",
        "husky": "9.1.7",
        "jest": "29.7.0",
        "jest-create-mock-instance": "2.0.0",
        "knip": "5.41.1",
        "lint-staged": "15.3.0",
        "prettier": "3.4.2",
        "semantic-release": "24.2.0",
        "ts-jest": "29.2.5",
        "ts-loader": "9.5.1",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.7.2",
        "yargs": "17.7.2"
    },
    "lint-staged": {
        "*.ts": "eslint --cache --fix",
        "*.{ts,js,css,md}": "prettier --write"
    },
    "release": {
        "branches": [
            "main"
        ]
    }
}
