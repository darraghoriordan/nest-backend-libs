{
    "name": "@darraghor/nest-backend-libs",
    "version": "1.0.1",
    "license": "SEE LICENSE IN LICENSE.md",
    "description": "Some helpers for personal projects in nestjs",
    "homepage": "https://github.com/darraghoriordan/nest-backend-libs",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/darraghoriordan/nest-backend-libs"
    },
    "engines": {
        "node": ">=22.13.1"
    },
    "files": [
        "dist",
        "package.json",
        "README.md"
    ],
    "author": {
        "name": "Darragh ORiordan",
        "email": "darragh.oriordan@gmail.com",
        "url": "https://darraghoriordan.com"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.js",
    "exports": {
        "import": "./dist/index.js"
    },
    "keywords": [
        "nestjs"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "bin": {
        "miller": "dist/cli/cli.js",
        "generate-client": "dist/open-api-generation/generate.sh"
    },
    "scripts": {
        "clean": "rm -Rf ./dist/",
        "build:ci": "pnpm run build",
        "build": "pnpm run clean && mkdir ./dist && tsc --project tsconfig.build.json && pnpm run copyOpenApiGenerationModule",
        "lint": "pnpm exec eslint",
        "lintone": "pnpm exec eslint  --no-error-on-unmatched-pattern",
        "test": "export NODE_OPTIONS=\"--max-old-space-size=8192\" && npx jest --runInBand --color --reporters=default --no-cache --coverage=false --passWithNoTests",
        "pre-commit": "npx lint-staged",
        "prettier": "prettier --write src/*",
        "prepare": "husky",
        "knip": "knip",
        "release": "npx semantic-release",
        "semantic-release": "semantic-release",
        "copyOpenApiGenerationModule": "copyfiles --error --up 1 \"src/open-api-generation/**/*\" \"src/open-api-generation/*\" \"src/database-initialisation/*\" dist"
    },
    "peerDependencies": {
        "@nestjs/axios": "4.0.0",
        "@nestjs/bull": "11.0.2",
        "@nestjs/common": "11.0.10",
        "@nestjs/config": "4.0.0",
        "@nestjs/core": "11.0.10",
        "@nestjs/microservices": "11.0.10",
        "@nestjs/passport": "11.0.5",
        "@nestjs/platform-express": "11.0.10",
        "@nestjs/swagger": "11.0.3",
        "@nestjs/terminus": "11.0.0",
        "@nestjs/typeorm": "11.0.0",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.1",
        "reflect-metadata": "0.2.2",
        "typeorm": "0.3.20"
    },
    "dependencies": {
        "@keyv/redis": "5.1.4",
        "@nestjs/axios": "4.0.1",
        "@nestjs/bull": "11.0.4",
        "@nestjs/cache-manager": "3.0.1",
        "@nestjs/common": "11.1.9",
        "@nestjs/config": "4.0.2",
        "@nestjs/core": "11.1.9",
        "@nestjs/microservices": "11.1.9",
        "@nestjs/passport": "11.0.5",
        "@nestjs/platform-express": "11.1.9",
        "@nestjs/swagger": "11.2.3",
        "@nestjs/terminus": "11.0.0",
        "@nestjs/typeorm": "11.0.0",
        "add": "2.0.6",
        "auth0": "5.1.0",
        "axios": "1.13.2",
        "bull": "4.16.5",
        "cache-manager": "7.2.6",
        "chalk": "5.6.2",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.3",
        "date-fns": "4.1.0",
        "dotenv": "17.2.3",
        "helmet": "8.1.0",
        "jwks-rsa": "3.2.0",
        "nanoid": "5.1.6",
        "nestjs-pino": "4.5.0",
        "nodemailer": "7.0.11",
        "passport": "0.7.0",
        "passport-headerapikey": "1.2.2",
        "passport-jwt": "4.0.1",
        "pino-http": "11.0.0",
        "pino-pretty": "13.1.3",
        "redis": "5.10.0",
        "reflect-metadata": "0.2.2",
        "rimraf": "6.1.2",
        "rxjs": "7.8.2",
        "stripe": "20.0.0",
        "swagger-ui-express": "5.0.1",
        "twitter-api-v2": "1.28.0",
        "typeorm": "0.3.28"
    },
    "devDependencies": {
        "@commitlint/cli": "20.2.0",
        "@commitlint/config-conventional": "20.2.0",
        "@darraghor/eslint-plugin-nestjs-typed": "6.9.17",
        "@eslint/js": "9.39.1",
        "@nestjs/cli": "11.0.14",
        "@nestjs/schematics": "^11.0.9",
        "@semantic-release/changelog": "6.0.3",
        "@semantic-release/exec": "7.1.0",
        "@types/auth0": "3.3.11",
        "@types/express": "5.0.6",
        "@types/jest": "30.0.0",
        "@types/node": "24.10.1",
        "@types/nodemailer": "7.0.4",
        "@types/passport-jwt": "4.0.1",
        "@types/supertest": "6.0.3",
        "@types/yargs": "17.0.35",
        "@typescript-eslint/eslint-plugin": "8.48.1",
        "@typescript-eslint/parser": "8.48.1",
        "copyfiles": "2.4.1",
        "eslint": "9.39.1",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-jest": "29.2.1",
        "eslint-plugin-sonarjs": "3.0.5",
        "eslint-plugin-unicorn": "62.0.0",
        "globals": "16.5.0",
        "husky": "9.1.7",
        "jest": "30.2.0",
        "jest-create-mock-instance": "2.0.0",
        "knip": "5.71.0",
        "lint-staged": "16.2.7",
        "prettier": "3.7.4",
        "semantic-release": "25.0.2",
        "ts-jest": "29.4.6",
        "ts-loader": "9.5.4",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.9.3",
        "typescript-eslint": "8.48.1",
        "yargs": "18.0.0"
    },
    "lint-staged": {
        "*.ts": "eslint --cache --fix",
        "*.{ts,js,css,md}": "prettier --write"
    },
    "release": {
        "branches": [
            "main"
        ]
    }
}
