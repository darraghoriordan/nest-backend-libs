{
    "name": "@darraghor/nest-backend-libs",
    "version": "1.0.1",
    "license": "SEE LICENSE IN LICENSE.md",
    "description": "Some helpers for personal projects in nestjs",
    "homepage": "https://github.com/darraghoriordan/nest-backend-libs",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/darraghoriordan/nest-backend-libs"
    },
    "engines": {
        "node": ">=v18.17.1"
    },
    "files": [
        "dist",
        "package.json",
        "README.md"
    ],
    "author": {
        "name": "Darragh ORiordan",
        "email": "darragh.oriordan@gmail.com",
        "url": "https://darraghoriordan.com"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.js",
    "exports": {
        "import": "./dist/index.js"
    },
    "keywords": [
        "nestjs"
    ],
    "bin": {
        "miller": "dist/cli/cli.js",
        "generate-client": "dist/open-api-generation/generate.sh"
    },
    "scripts": {
        "clean": "rm -Rf ./dist/",
        "build:ci": "yarn build",
        "build": "yarn clean && mkdir ./dist && tsc --project tsconfig.build.json && yarn copyOpenApiGenerationModule",
        "lint": "npx eslint -c .eslintrc.cjs --ext .ts './src/**/*.ts' './tests/**/*.ts' --no-error-on-unmatched-pattern",
        "test": "export NODE_OPTIONS=\"--max-old-space-size=8192\" && npx jest --runInBand --color --reporters=default --no-cache --coverage=false --passWithNoTests",
        "pre-commit": "npx lint-staged",
        "prettier": "prettier --write src/*",
        "prepare": "husky install",
        "knip": "knip",
        "release": "npx semantic-release",
        "semantic-release": "semantic-release",
        "copyOpenApiGenerationModule": "copyfiles --error --up 1 \"src/open-api-generation/**/*\" \"src/open-api-generation/*\" \"src/database-initialisation/*\" dist"
    },
    "peerDependencies": {
        "@nestjs/axios": "2.0.0",
        "@nestjs/bull": "0.6.3",
        "@nestjs/common": "9.3.12",
        "@nestjs/config": "2.3.1",
        "@nestjs/core": "9.3.12",
        "@nestjs/microservices": "9.3.12",
        "@nestjs/passport": "9.0.3",
        "@nestjs/platform-express": "9.3.12",
        "@nestjs/swagger": "6.2.1",
        "@nestjs/terminus": "9.2.1",
        "@nestjs/typeorm": "9.0.1",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.0",
        "reflect-metadata": "0.1.13",
        "typeorm": "0.3.12"
    },
    "dependencies": {
        "@nestjs/axios": "3.0.0",
        "@nestjs/bull": "10.0.1",
        "@nestjs/cache-manager": "2.1.0",
        "@nestjs/common": "10.2.7",
        "@nestjs/config": "3.1.1",
        "@nestjs/core": "10.2.7",
        "@nestjs/microservices": "10.2.7",
        "@nestjs/passport": "10.0.2",
        "@nestjs/platform-express": "10.2.7",
        "@nestjs/swagger": "7.1.13",
        "@nestjs/terminus": "10.1.1",
        "@nestjs/typeorm": "10.0.0",
        "add": "2.0.6",
        "auth0": "4.0.1",
        "axios": "1.5.1",
        "bull": "4.11.3",
        "cache-manager": "5.2.4",
        "cache-manager-redis-yet": "4.1.2",
        "chalk": "5.3.0",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.0",
        "date-fns": "2.30.0",
        "dotenv": "16.3.1",
        "helmet": "7.0.0",
        "jwks-rsa": "3.1.0",
        "nanoid": "5.0.1",
        "nestjs-pino": "3.5.0",
        "nodemailer": "6.9.6",
        "passport": "0.6.0",
        "passport-headerapikey": "1.2.2",
        "passport-jwt": "4.0.1",
        "pino-http": "8.5.0",
        "pino-pretty": "10.2.3",
        "redis": "4.6.10",
        "reflect-metadata": "0.1.13",
        "rimraf": "5.0.5",
        "rxjs": "7.8.1",
        "stripe": "13.10.0",
        "swagger-ui-express": "5.0.0",
        "twitter-api-v2": "1.15.2",
        "typeorm": "0.3.17"
    },
    "devDependencies": {
        "@commitlint/cli": "17.7.2",
        "@commitlint/config-conventional": "17.7.0",
        "@darraghor/eslint-plugin-nestjs-typed": "4.2.1",
        "@nestjs/cli": "10.1.18",
        "@nestjs/schematics": "^10.0.2",
        "@semantic-release/changelog": "6.0.3",
        "@semantic-release/exec": "6.0.3",
        "@types/auth0": "3.3.6",
        "@types/express": "4.17.19",
        "@types/jest": "29.5.5",
        "@types/node": "20.8.4",
        "@types/nodemailer": "6.4.11",
        "@types/passport-jwt": "3.0.10",
        "@types/supertest": "2.0.14",
        "@typescript-eslint/eslint-plugin": "6.7.5",
        "@typescript-eslint/parser": "6.7.5",
        "copyfiles": "2.4.1",
        "eslint": "8.51.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-jest": "27.4.2",
        "eslint-plugin-jsdoc": "46.8.2",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-promise": "6.1.1",
        "eslint-plugin-sonarjs": "0.21.0",
        "eslint-plugin-unicorn": "48.0.1",
        "husky": "8.0.3",
        "jest": "29.7.0",
        "jest-create-mock-instance": "2.0.0",
        "knip": "2.33.1",
        "lint-staged": "14.0.1",
        "prettier": "3.0.3",
        "semantic-release": "22.0.5",
        "ts-jest": "29.1.1",
        "ts-loader": "9.5.0",
        "ts-node": "10.9.1",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.2.2"
    },
    "lint-staged": {
        "*.ts": "eslint --cache --fix",
        "*.{ts,js,css,md}": "prettier --write"
    },
    "release": {
        "branches": [
            "main"
        ]
    }
}
