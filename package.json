{
    "name": "@darraghor/nest-backend-libs",
    "version": "1.0.1",
    "license": "UNLICENSED",
    "description": "Some helpers for personal projects",
    "homepage": "https://github.com/darraghoriordan/nest-backend-libs",
    "repository": {
        "type": "git",
        "url": "https://github.com/darraghoriordan/nest-backend-libs"
    },
    "engines": {
        "node": ">=12"
    },
    "files": [
        "dist",
        "package.json",
        "README.md"
    ],
    "author": {
        "name": "Darragh ORiordan",
        "email": "darragh.oriordan@gmail.com",
        "url": "https://darraghoriordan.com"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "keywords": [
        "nestjs"
    ],
    "scripts": {
        "clean": "rm -Rf ./dist/",
        "build:ci": "yarn build",
        "build": "yarn clean && mkdir ./dist && tsc --project tsconfig.build.json && yarn copyOpenApiGenerationModule",
        "lint": "npx eslint -c .eslintrc.js --ext .ts './src/**/*.ts' './tests/**/*.ts' --no-error-on-unmatched-pattern",
        "test": "export NODE_OPTIONS=\"--max-old-space-size=8192\" && npx jest --runInBand --color --reporters=default --no-cache --coverage=false --passWithNoTests",
        "pre-commit": "npx lint-staged",
        "prettier": "prettier --write src/*",
        "prepare": "husky install",
        "release": "npx semantic-release",
        "semantic-release": "semantic-release",
        "copyOpenApiGenerationModule": "copyfiles --error --up 1 \"src/open-api-generation/**/*\" \"src/open-api-generation/*\" \"src/database-initialisation/*\" dist"
    },
    "dependencies": {
        "@nestjs/common": "9.2.0",
        "@nestjs/config": "2.2.0",
        "@nestjs/core": "9.2.0",
        "@nestjs/passport": "9.0.0",
        "@nestjs/platform-express": "9.2.0",
        "@nestjs/swagger": "6.1.3",
        "@nestjs/typeorm": "9.0.1",
        "auth0": "2.44.0",
        "class-transformer": "0.5.1",
        "class-validator": "0.13.2",
        "date-fns": "2.29.3",
        "dotenv": "16.0.3",
        "jwks-rsa": "3.0.0",
        "nodemailer": "6.8.0",
        "passport": "0.6.0",
        "passport-jwt": "4.0.0",
        "pg": "8.8.0",
        "reflect-metadata": "0.1.13",
        "res": "^0.4.0",
        "resource": "0.8.1",
        "rimraf": "3.0.2",
        "rxjs": "7.5.7",
        "swagger-ui-express": "4.6.0",
        "tslog": "4.2.0",
        "typeorm": "^0.3.10"
    },
    "devDependencies": {
        "@commitlint/cli": "17.2.0",
        "@commitlint/config-conventional": "17.2.0",
        "@darraghor/eslint-plugin-nestjs-typed": "3.15.2",
        "@nestjs/cli": "9.1.5",
        "@nestjs/schematics": "^9.0.3",
        "@semantic-release/changelog": "6.0.1",
        "@semantic-release/exec": "6.0.3",
        "@types/auth0": "2.35.7",
        "@types/express": "4.17.14",
        "@types/jest": "29.2.3",
        "@types/node": "18.11.9",
        "@types/nodemailer": "6.4.6",
        "@types/passport-jwt": "3.0.7",
        "@types/rimraf": "3.0.2",
        "@types/supertest": "2.0.12",
        "@typescript-eslint/eslint-plugin": "5.43.0",
        "@typescript-eslint/parser": "5.43.0",
        "copyfiles": "2.4.1",
        "eslint": "8.27.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-jest": "27.1.5",
        "eslint-plugin-jsdoc": "39.6.2",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-promise": "6.1.1",
        "eslint-plugin-sonarjs": "0.16.0",
        "eslint-plugin-unicorn": "44.0.2",
        "husky": "8.0.2",
        "jest": "29.3.1",
        "jest-create-mock-instance": "2.0.0",
        "lint-staged": "13.0.3",
        "prettier": "2.7.1",
        "semantic-release": "19.0.5",
        "ts-jest": "29.0.3",
        "ts-loader": "9.4.1",
        "ts-node": "10.9.1",
        "tsconfig-paths": "4.1.0",
        "typescript": "4.9.3"
    },
    "lint-staged": {
        "*.ts": "eslint --cache --fix",
        "*.{ts,js,css,md}": "prettier --write"
    },
    "release": {
        "branches": [
            "main"
        ]
    }
}
